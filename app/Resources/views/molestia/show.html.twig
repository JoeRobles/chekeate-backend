{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Molestia</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ molestium.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ molestium.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ molestium.descripcion }}</td>
            </tr>
            <tr>
                <th>Keywords</th>
                <td>{{ molestium.keywords }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('molestia_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('molestia_edit', { 'id': molestium.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
